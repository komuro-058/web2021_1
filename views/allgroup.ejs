<!Doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>Show message</title>
</head>
<body>
  <h2>グループ一覧</h2>
   <form action ="/" >
    <input class="icon" type="submit" value="TOP">
  </form>
  <form action="/public/gsort.html">
    <input class="icon" type="submit" value="並べ替え"> 
  </form>
  <table border=1>
    <tr>
        <th></th>
        <th>グループ名</th>
        <th>ファミリークラブ会員数</th>
        <th>デビュー日</th>
    </tr>
      <% for( let row of data ) { %>
        
        <tr>
            <td><a href="/team/<%=row.team_id%>"><%= row.team_id %></a></td>
            <td><%= row.team %></td>
            <td><%= row.funs +"名" %></td>
            <td><%= row.debut %></td>
        </tr>
      <% } %>
  </table>
        
 <form action="/public/insert.html">
   <input type="submit" value="追加">
 </form>
</body>
</html>