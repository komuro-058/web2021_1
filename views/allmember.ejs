<!Doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>Show message</title>
</head>
<body>
  <h2>タレント一覧</h2>
  <form action="/">
    <input type="submit" value="TOP">
  </form>
  <table border=1>
    <tr>
        <td></td>
        <td>名前</td>
        <td>生年月日</td>
        <td>出身</td>
        <td>所属グループ</td>
    </tr>
      <% for( let row of data ) { %>
        
        <tr>
            <td><a href="/jidol/<%=row.id%>"><%= row.id %></a></td>
            <td><%= row.name %></td>
            <td><%= row.birthday %></td>
            <td><%= row.birthplace %></td>
            <td><%= row.team %></td>
        </tr>
      <% } %>
  </table>

  <form action="/public/insert2.html">
    <input type="submit" value="追加">
  </form>
</body>
</html>